{
  "name": "polar-net-support",
  "version": "1.0.0",
  "description": "Polar.net \u2014 Telecom customer support demo (NestJS + WebSockets).",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
